{
    "name": "yudistira/satusehat-api",
    "description": "Non Official SATUSEHAT API with FHIR Object in Easy Way",
    "keywords": [
        "yudistirasd",
        "satusehat",
        "satusehat-api",
        "rest-api",
        "satusehat-rest-api"
    ],
    "homepage": "https://github.com/yudistirasd/satusehat-api",
    "license": "MIT",
    "authors": [
        {
            "name": "yudistirasd",
            "email": "yudistira.sd2@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/config": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "jeroenzwart/laravel-csv-seeder": "^1.6",
        "phpseclib/phpseclib": "^3.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "suggest": {
        "laravel/pint": "Minimalist Opionated Code Formatting Extension for PHP 8.0+",
        "pestphp/pest": "Pest PHP Testing Framework for PHP 8.0+"
    },
    "autoload": {
        "psr-4": {
            "Satusehat\\Integration\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Satusehat\\Integration\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Satusehat\\Integration\\SatusehatIntegrationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
